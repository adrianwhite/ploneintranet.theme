<?xml version="1.0"?>
<object name="portal_css" meta_type="Stylesheets Registry">
  <stylesheet title=""
              cacheable="True"
              compression="safe"
              cookable="True"
              enabled="1"
              id="++theme++ploneintranet.theme/generated/style/screen.css"
              media="screen"
              rel="stylesheet"
              rendering="import"
              expression="python:request.getHeader('X-Theme-Enabled') and not request.response.getHeader('X-Theme-Disabled')"
              applyPrefix="True" />
